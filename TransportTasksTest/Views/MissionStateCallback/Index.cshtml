@{
    var isButtonEnabled = ViewBag.IsButtonEnabled ?? false;
}

                    
<h4>Mission State Callback</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Go">
            <div class="form-group">
                <input type="submit" value="GO" class="btn btn-danger" @(isButtonEnabled ? "" : "disabled") />
            </div>
        </form>
    </div>
</div>
